{
  "name": "dpzvc3-ui",
  "version": "3.1.6",
  "description": "Vue3组件库,基于Vue3+Webpack5+TypeScript开发，提供丰富的移动端UI组件。",
  "main": "dist/dpzvc3.js",
  "module": "dist/dpzvc3.esm.js",
  "types": "dist/types/index.d.ts",
  "scripts": {
    "init": "webpack --progress --config webpack.dev.config.js",
    "dev": "webpack serve --config webpack.dev.config.js --open",
    "build:ssr": "webpack --config webpack.client.config.js && webpack --config webpack.server.config.js",
    "build:style": "gulp --gulpfile build-style.js",
    "build:dev": "webpack --config webpack.dist.dev.config.js",
    "build:prod": "webpack --config webpack.dist.prod.config.js",
    "build:types": "tsc --project tsconfig.build.json && cp src/index.d.ts dist/types/index.d.ts",
    "build": "npm run build:style && npm run build:dev && npm run build:prod && npm run build:types",
    "dist": "webpack --progress --config webpack.prod.config.js",
    "prepare": "husky",
    "start": "node server.js",
    "vercel-build": "npm run build:ssr"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yishide/dpzvc3.git"
  },
  "files": [
    "dist",
    "README.md",
    "images"
  ],
  "keywords": [
    "Vue3",
    "Vue3 Cli",
    "Webpack5",
    "dpzvc3",
    "UI",
    "Vue3 Components",
    "Vue3组件库",
    "移动端组件库"
  ],
  "dependencies": {
    "china-area-data": "^3.0.0",
    "core-js": "^2.4.1",
    "css-loader": "^7.1.2",
    "deepmerge": "^1.3.1",
    "exifr": "^7.1.3",
    "express": "^5.2.1",
    "less": "^4.5.1",
    "less-loader": "^12.3.0",
    "path-to-regexp": "^6.3.0",
    "postcss-loader": "^8.2.0",
    "vue": "^3.5.26",
    "vue-router": "^4.6.4",
    "vue-style-loader": "^4.1.3",
    "webpack": "^5.104.1",
    "webpack-cli": "^6.0.1",
    "webpack-merge": "^6.0.1",
    "webpack-node-externals": "^3.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/plugin-transform-runtime": "^7.28.5",
    "@babel/preset-env": "^7.28.5",
    "@babel/preset-typescript": "^7.28.5",
    "@babel/runtime": "^7.28.4",
    "@typescript-eslint/eslint-plugin": "^8.51.0",
    "@typescript-eslint/parser": "^8.51.0",
    "@vue/babel-plugin-jsx": "^2.0.1",
    "@vue/compiler-sfc": "^3.5.26",
    "@vue/eslint-config-standard": "^8.0.1",
    "autoprefixer": "^10.4.23",
    "autoprefixer-loader": "^2.0.0",
    "babel": "^6.23.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.4.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-latest": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "cross-env": "^3.1.4",
    "del": "^8.0.1",
    "eslint": "^8.57.1",
    "eslint-plugin-html": "^1.7.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.6.0",
    "eslint-plugin-vue": "^9.33.0",
    "eslint-webpack-plugin": "^4.2.0",
    "file-loader": "^0.8.5",
    "function-bind": "^1.1.0",
    "gh-pages": "^6.3.0",
    "gulp": "^4.0.2",
    "gulp-clean-css": "^4.3.0",
    "gulp-cli": "^2.3.0",
    "gulp-header": "^2.0.9",
    "gulp-less": "^5.0.0",
    "gulp-postcss": "^10.0.0",
    "gulp-rename": "^2.1.0",
    "html-loader": "^0.3.0",
    "html-webpack-plugin": "^5.6.5",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "lolex": "^1.5.2",
    "mini-css-extract-plugin": "^2.9.4",
    "postcss": "^8.5.6",
    "style-loader": "^3.3.4",
    "terser-webpack-plugin": "^4.2.3",
    "ts-loader": "^9.5.4",
    "typescript": "^5.9.3",
    "url-loader": "^0.5.9",
    "vue-eslint-parser": "^10.2.0",
    "vue-hot-reload-api": "^1.3.3",
    "vue-html-loader": "^1.2.3",
    "vue-loader": "^17.4.2",
    "webpack-dev-server": "^4.15.2"
  },
  "author": "yishide",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yishide/dpzvc3/issues"
  },
  "homepage": "https://github.com/yishide/dpzvc3#readme",
  "directories": {
    "example": "example"
  },
  "lint-staged": {
    "*.{js,vue}": [
      "eslint --max-warnings=0"
    ]
  }
}
